{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["./dist/**.*", "./build/**.*"]
    },
    "publish": {
      "dependsOn": ["build"],
      "inputs": ["./dist/**.*", "./build/**.*"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
